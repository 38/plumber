package plumber.std.numeric;

/* The base type used to represent a device memory object */
type DeviceObject {
	request_local_token    device_obj@DeviceObject;   /*!< The memory object that lives on the device */
};

/* The object lives on OpenCL device */
type OpenCL : DeviceObject {};

